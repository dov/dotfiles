# override urls, specific to nwlc
from pricing.views import typed_fees
from pricing.models import BFF, BHF, BF, SF
from django.conf.urls.defaults import patterns, url



override_urls = patterns('pricing.views',
    #url(r'^all_fees/$', typed_fees, name='all_fees'),
    url(r'^barristers_fixed_fees/$', typed_fees, kwargs={'manager':BFF}, name=BFF),
    url(r'^barristers_hourly_fees/$', typed_fees, kwargs={'manager':BHF}, name=BHF),
    url(r'^barristers_fees/$', typed_fees, kwargs={'manager':BF}, name=BF),
    url(r'^solicitors_fees/$', typed_fees, kwargs={'manager':SF}, name=SF),
    (r'^method_statements_list/$', 'method_statement_templates'),
    (r'^method_statements_for_panel/(\d+)/$', 'method_statements_for_template'),
    (r'^method_statement_view/(\d+)/$', 'method_statement_view'),
)
